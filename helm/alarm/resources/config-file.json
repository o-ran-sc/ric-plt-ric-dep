{
    "local": {
        "host": ":8080"
    },
    "logger": {
        "level": 4
    },
    "db": {
        "namespaces": ["sdl", "rnib"]
    },
    "rmr": {
        "protPort": "tcp:4560",
        "maxSize": 1024,
        "numWorkers": 1
    },
    "promAlertManager": {
        "address": "{{ .Values.alarm.promAlertManager.address }}",
        "baseUrl": "{{ .Values.alarm.promAlertManager.baseUrl }}",
        "schemes": "http",
        "alertInterval": 30000
    },
    "alarmDefinitions": [
        {
            "specificProblem": 1234,
            "Text": "Connection to DBAAS server lost"
        },
        {
            "specificProblem": 1235,
            "Text": "Distribution of internal routing table failed due to timeout"
        },
        {
            "specificProblem": 1236,
            "Text": "E2 connection to gNB lost"
        }
    ]
}
